<script>
    function toggleCollapse(btn) {
        const card = btn.closest('.custom-card');
        const body = card.querySelector('.custom-card-body');
        const icon = btn.querySelector('i');

        const isCollapsed = !body.classList.contains('open');

        if (isCollapsed) {
            // EXPAND
            body.classList.add('expanding');
            body.style.height = body.scrollHeight + 'px';
            body.classList.add('open');
            icon.classList.remove('fa-plus');
            icon.classList.add('fa-minus');

            body.addEventListener('transitionend', function expandDone(e) {
                if (e.propertyName === 'height') {
                    body.style.height = 'auto';
                    body.classList.remove('expanding');
                    body.removeEventListener('transitionend', expandDone);
                }
            });
        } else {
            // COLLAPSE
            body.style.height = body.scrollHeight + 'px'; // set current height before collapse
            requestAnimationFrame(() => {
                body.style.height = '0px';
                body.classList.add('collapsing');
                body.classList.remove('open');
                icon.classList.remove('fa-minus');
                icon.classList.add('fa-plus');
            });

            body.addEventListener('transitionend', function collapseDone(e) {
                if (e.propertyName === 'height') {
                    body.classList.remove('collapsing');
                    body.removeEventListener('transitionend', collapseDone);
                }
            });
        }
    }
</script>
